def ns_block<101> = proto {
   extract into gb;
   list<gbe> ls_elements;
   stream gbe gbe;
   list<gbf> ls_fields;
   stream gbf gbf;

   int data_len @ P(1);

   APP <- ""[
   nbe.cdef[[
      #include "nbe_common.h"
      #include "dict.h"
      #include "gb_l3_common.h"
      #include "stdio.h"

      BEGIN_SIGNAL("1", "NS Layer", "NS-BLOCK");

      ns_pdu_type();
      ns_cause();
      ns_vci();
      if (MYVAR(data_len) - 8 > 0)
      {
	      uint32_t pos = 0;
          POS(pos);
          SET_V(gb, next_pos, pos);
          SET_V(gb, next_len, MYVAR(data_len) - 8);
      }

      END_SIGNAL();
   ]]
   ];
};
