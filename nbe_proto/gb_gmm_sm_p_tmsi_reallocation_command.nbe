def p_tmsi_reallocation_command<101> = proto {
   extract into gb;
   list<gbe> ls_elements;
   stream gbe gbe;
   list<gbf> ls_fields;
   stream gbf gbf;

   int dir @ P(1);

  APP <- ""[
  nbe.cdef[[
	#include "nbe_common.h"
	#include "dict.h"
	#include "gb_l3_common.h"
	#include "stdio.h"
    #include "gb_gmm_sm.h"

	BEGIN_SIGNAL("4", "GMM", "P-TMSI REALLOCATION COMMAND");
	

	uint8_t iei = 0;
	uint8_t location = 0;
	uint8_t h = 0;

	si_pd();
	msg_type(MESSAGE_TYPE_GMM);
	mobile_identity(iei, location);
	//routing_area_identification(iei, location);
    routeing_area(iei, location);
	
	spare_half_octet(iei, location);
    location = 1;
	force_to_standby(iei, location);
	
	B8_BIT(h, 1, 8);
	if(h == 0x19)
	{
		p_tmsi_signature_2(iei, location);
	}
	END_SIGNAL();
  ]]
  ];
};
